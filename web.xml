<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Registrationjsp</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginControler</servlet-name>
    <servlet-class>org.oecm19.Controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginControler</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>employeeRegisteration</servlet-name>
    <servlet-class>org.oecm19.Controller.EmployeeRegistration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>employeeRegisteration</servlet-name>
    <url-pattern>/employeeregistr</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>searchEmployee</servlet-name>
    <servlet-class>org.oecm19.Controller.SearchEmpDetails</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>searchEmployee</servlet-name>
    <url-pattern>/searchE</url-pattern>
  </servlet-mapping>
</web-app>